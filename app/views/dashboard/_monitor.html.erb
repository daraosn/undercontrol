<div class="row">
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <%= icon 'clock-o'%>
                Real Time Monitor (last {{ realtimeLastMinutes }} mins)
            </div>
            <div class="panel-body">
                <div class="flot-chart" style="height: 300px">
                    <div class="realtime-chart flot-chart-content"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <%= icon 'history'%>
                Historic Chart (last <span>{{ historicChartRange }}</span>)
                <div class="pull-right">
                    <select class="" ng-options="k as v for (k, v) in historicChartRanges" ng-model="historicChartRange" ng-change="changehistoricChartRange()"></select>
                </div>
            </div>
            <div class="panel-body">
                <div class="historic-chart dygraph-area-chart" style="height: 300px"></div>
            </div>
        </div>
    </div>
</div>
